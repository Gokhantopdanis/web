@using MvcKutuphane.Models.Entity
@model List<TBLDUYURULAR>

@foreach (var d in Model)
{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/2.png" alt="user image">
            <span class="username">
                <a href="#">@d.KATEGORI</a>
                <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>
            </span>
            <span class="description">@d.TARIH.ToString()</span>
        </div>
        <p>
            @d.ICERIK
        </p>
    </div>
}